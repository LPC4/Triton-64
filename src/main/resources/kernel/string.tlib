func strcmp(str1: long, len1: int, str2: long, len2: int): int {
    if (len1 != len2) {
        return 0
    }

    var i: int = 0
    var equal: int = 1
    while (i < len1 && equal == 1) {
        if (byte@(str1 + i) != byte@(str2 + i)) {
            equal = 0
        }
        i = i + 1
    }
    return equal
}