import memory

func main(): long {
    var arr2d: long[][] = calloc(3, strideOf(long[]))
    var arr1: long[] = calloc(3, strideOf(long))
    var arr2: long[] = calloc(3, strideOf(long))
    @arr1 = [1, 2, 3] ; store first row
    @arr2 = [4, 5, 6] ; store second row
    @arr2d = [arr1, arr2, arr1] ; store array

    var sum: int = 0
    var i: byte = 0
    while (i < 3) {
        var j: byte = 0
        while (j < 3) {
            sum = sum + arr2d[i][j] ; sum all elements
            j = j + 1
        }
        i = i + 1
    }
    free(arr2d) ; free the allocated memory

    return long sum ; return the total sum of all elements
}
