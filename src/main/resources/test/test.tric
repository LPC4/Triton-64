func main() {
    var a = 20
    var b = factorial(5)  ; 120
    var d = 0
    var c = 0

    c = subbig(a, b) ; 100

    while (c < 1000) { ; d = 900
        d = add(d, 1)
        c = c + 1
    }

    return d ; a0 = 900
}

func add(x, y) {
    return x + y
}

func subbig(x, y) {
    if (x < y) {
        return y - x
    } else {
        return x - y
    }
}

func factorial(n) {
    if (n <= 1) {
        return 1
    } else {
        return n * factorial(n - 1)
    }
}